            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/etoilefond" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="113dp"
                android:layout_height="115dp"
                app:srcCompat="@drawable/asteroid1"
                tools:layout_editor_absoluteX="129dp"
                tools:layout_editor_absoluteY="40dp" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="90dp"
                android:layout_height="86dp"
                app:srcCompat="@drawable/asteroid2"
                tools:layout_editor_absoluteX="269dp"
                tools:layout_editor_absoluteY="142dp" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="62dp"
                android:layout_height="67dp"
                app:srcCompat="@drawable/asteroid3"
                tools:layout_editor_absoluteX="38dp"
                tools:layout_editor_absoluteY="293dp" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="128dp"
                android:layout_height="87dp"
                app:srcCompat="@drawable/asteroid4"
                tools:layout_editor_absoluteX="231dp"
                tools:layout_editor_absoluteY="280dp" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="155dp"
                android:layout_height="165dp"
                app:srcCompat="@drawable/tie"
                tools:layout_editor_absoluteX="119dp"
                tools:layout_editor_absoluteY="405dp" />

            <FrameLayout
                android:layout_width="397dp"
                android:layout_height="174dp"
                tools:layout_editor_absoluteX="-4dp"
                tools:layout_editor_absoluteY="611dp">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/pad_center" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/pad_exterior" />
            </FrameLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>